using namespace std;

 class Verticals{
    double x, y, z;
 };
 class normali{
     double a, b, c;
 };
 class tex_cord{
     double x, y;
 };
 class facels{
     double A[4], B[4], C[4];
 };
 class count{
     public:
     unsigned int normal_count = 0, facels_count = 0, tex_cord_count = 0, verticals_count =0;
     void Count(char* bufer, FILE* stream, int i, char* determinant_tr){
        while(i <= 40){
            fgets(bufer, sizeof(bufer), stream);
            if(bufer[0] == determinant_tr[0]){
                verticals_count++;
            }
            if(bufer[0] == determinant_tr[1]){
                tex_cord_count++;
            }
            if(bufer[0] == determinant_tr[2]){
                normal_count++;
            }
            if(bufer[0] == determinant_tr[3]){
                facels_count++;
            }
            i++;
            cout << verticals_count;
        }
    }
};

int main()
{
    FILE *stream;
    stream = fopen("cube.obj", "r");
    char bufer[100];
    //bufer.c_str;
    vector<string> determinant;
    char determinant_tr[8];
    string y;
    for(int i = 0; i<4; i++){
        cin >> y;
        determinant.push_back(y);   
    }
    determinant_tr[0] = determinant[0][0];
    determinant_tr[1] = determinant[1][0];
    determinant_tr[2] = determinant[2][0];
    determinant_tr[3] = determinant[3][0];
    count c;
    //cout << c.normal_count;
    
    int i = 0;
    //while(i<sizeof(bufer)){
    //    std::cout << bufer[i] << std::endl;
    //    i++;
